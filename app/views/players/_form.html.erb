<%= simple_form_for(@player) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :attendee %>
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.association :game_event %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
